#setup

su -c 'apt-get build-essential'

su -c 'apt-get bison gettext flex texinfo unzip'
su -c 'apt-get ncurses-dev'

echo 'export PRJROOT=/mnt/gateway' >> ~/.bashrc 

mkdir ${PRJROOT}/source
echo 'export PRJSOURCE=${PRJROOT}/source' >> ~/.bashrc 

mkdir ${PRJROOT}/configs
echo 'export PRJCONFIGS=${PRJROOT}/configs' >> ~/.bashrc 

source ~/.bashrc

#download

cd  ${PRJROOT}/source
wget http://buildroot.net/downloads/buildroot-2011.08.tar.bz2

#unpack
tar xvf ./buildroot-2011.08.tar.bz2

#configure 
tar xvf ./buildroot-configs.tar
cp ${CONFIGDIR}
#build
cd ./buildroot-2011.08
make menuconfig


